extends ../layouts/default

block content
	.container.store-container
		.store-content
			if data.events.length			
				each event in data.events
					h1
						a(href='/events/' + event.slug)=	event.title
					- var  displayLocation = event.location.suburb +','+  event.location.state
					- if(displayLocation.indexOf('undefined')!==-1){displayLocation=''}
					p=  displayLocation
					- var date = moment(event.datetime).format('MM/DD/YYYY')
					p=	date
					- var time = moment(event.datetime).format('hh:mm a')
					p= time
			else